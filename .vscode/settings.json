{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "eslint.run": "onSave",
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact"
  ],
  "eslint.workingDirectories": [
    "./"
  ],
  "eslint.options": {
    "overrideConfigFile": "./.eslintrc.json"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
}